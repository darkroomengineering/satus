    "react-dom": "^19.1.0",
    "react-use": "^17.6.0",
    "storyblok-js-client": "^7.0.0",
    "storyblok-rich-text-react-renderer": "^3.0.1",
    "tempus": "1.0.0-dev.11",
    "three": "^0.177.0",
    "tunnel-rat": "^0.1.2",
    "web-vitals": "^5.0.3",
    "zustand": "^5.0.5"
