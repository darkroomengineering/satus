{
  "name": "@darkroom.engineering/satus",
  "description": "SatÅ«s is a next.js starter",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"NXT,CSS\" -c \"bgWhiteBright.bold,bgRed.bold\" \"bun dev:next\" \"bun watch:styles\" --kill-others",
    "dev:next": "next dev --turbopack",
    "dev:https": "next dev --turbopack --experimental-https",
    "build": "next build --turbopack",
    "build:analyze": "cross-env SOURCE_MAPS=true bun run build && source-map-explorer .next/static/chunks/*.js",
    "build:profile": "cross-env NEXT_BUILD_PROFILE=true next build",
    "wp:build": "next build",
    "start": "next start",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format --write .",
    "analyze": "cross-env ANALYZE=true next build",
    "typecheck": "tsc --noEmit --incremental false",
    "setup:styles": "bun ./styles/scripts/setup-styles.ts",
    "watch:styles": "bun --watch ./styles/scripts/setup-styles.ts",
    "lighthouse": "bunx @unlighthouse/cli --site http://localhost:3000"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.0",
    "@hubspot/api-client": "^13.0.0",
