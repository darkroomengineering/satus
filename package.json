{
  "name": "@darkroom.engineering/satus",
  "description": "SatÅ«s is a next.js starter",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "bun ./lib/scripts/dev.ts",
    "dev:https": "bun ./lib/scripts/dev.ts --https",
    "build": "bun run setup:styles && next build",
    "start": "next start",
    "lint": "biome lint --max-diagnostics=200",
    "lint:fix": "biome lint --write --unsafe --max-diagnostics=200",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit --incremental false",
    "analyze": "cross-env ANALYZE=true bun run build",
    "lighthouse": "bunx @unlighthouse/cli --site http://localhost:3000",
    "setup:project": "bun ./lib/scripts/setup-project.ts",
    "setup:styles": "bun ./lib/styles/scripts/setup-styles.ts",
    "cleanup:integrations": "bun ./lib/integrations/cleanup.ts",
    "create": "bun ./lib/scripts/create.ts",
    "sanity:extract": "cd lib/integrations/sanity && bun --env-file ../../../.env.local sanity schema extract",
    "sanity:typegen": "cd lib/integrations/sanity && bun --env-file ../../../.env.local sanity typegen generate && bun biome check --write --unsafe",
    "sanity:mcp": "bun ./lib/scripts/copy-sanity-mcp.ts"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-rc.0",
    "@portabletext/react": "^6.0.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@sanity/asset-utils": "^2.3.0",
    "@sanity/image-url": "^2.0.2",
    "@sanity/visual-editing": "^5.0.3",
    "clsx": "^2.1.1",
    "gsap": "^3.14.2",
    "hamo": "1.0.0-dev.6",
    "lenis": "^1.3.16",
    "next": "16.0.10",
    "next-sanity": "^12.0.1",
    "postprocessing": "^6.38.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tempus": "1.0.0-dev.17",
    "three": "^0.182.0",
    "tunnel-rat": "^0.1.2",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@clack/prompts": "^0.11.0",
    "@csstools/postcss-global-data": "^3.1.0",
    "@hubspot/api-client": "^13.4.0",
    "@next/bundle-analyzer": "16.0.10",
    "@sanity/vision": "^5.0.1",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@theatre/core": "^0.7.2",
    "@theatre/studio": "^0.7.2",
    "@types/bun": "^1.3.4",
    "@types/node": "^25.0.3",
    "@types/react": "19.2.7",
    "@types/three": "^0.182.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "lefthook": "^2.0.12",
    "postcss-functions": "^4.0.2",
    "postcss-preset-env": "^10.5.0",
    "sanity": "^5.0.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@tailwindcss/oxide",
    "esbuild",
    "lefthook",
    "sharp"
  ],
  "packageManager": "bun@1.3.0"
}
