{
  "name": "@darkroom.engineering/satus",
  "description": "SatÅ«s is a next.js starter",
  "version": "0.0.2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"NXT,CSS\" -c \"bgWhiteBright.bold,bgRed.bold\" \"bun dev:next\" \"bun watch:styles\" --kill-others",
    "dev:next": "next dev --turbopack",
    "dev:https": "next dev --turbopack --experimental-https",
    "build": "next build --turbopack",
    "build:analyze": "cross-env SOURCE_MAPS=true bun run build && source-map-explorer .next/static/chunks/*.js",
    "build:profile": "cross-env NEXT_BUILD_PROFILE=true next build",
    "start": "next start",
    "lint": "biome lint --max-diagnostics=200",
    "lint:fix": "biome lint --write --max-diagnostics=200",
    "format": "biome format --write .",
    "analyze": "cross-env ANALYZE=true next build",
    "typecheck": "tsc --noEmit --incremental false",
    "validate:env": "bun ./libs/validate-env.ts",
    "cleanup:integrations": "bun ./libs/cleanup-integrations.ts",
    "setup:styles": "bun ./styles/scripts/setup-styles.ts",
    "watch:styles": "bun --watch ./styles/scripts/setup-styles.ts",
    "lighthouse": "bunx @unlighthouse/cli --site http://localhost:3000",
    "setup:sanity-mcp": "echo -n 'cursor://anysphere.cursor-deeplink/mcp/install?name=Sanity&config=eyJ1cmwiOiJodHRwczovL21jcC5zYW5pdHkuaW8iLCJ0eXBlIjoiaHR0cCJ9Cg==' | pbcopy && echo 'Link copied to clipboard (without newline)' && echo 'Paste this in Cursor or browser address bar'",
    "sanity:schema-extract": "cd integrations/sanity && bun --env-file ../../.env.local sanity schema extract",
    "sanity:typegen": "cd integrations/sanity && bun --env-file ../../.env.local sanity typegen generate"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.4",
    "@hubspot/api-client": "^13.4.0",
    "@next/third-parties": "^15.5.4",
    "@portabletext/react": "^4.0.3",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@sanity/asset-utils": "^2.3.0",
    "@sanity/image-url": "^1.2.0",
    "@sanity/visual-editing": "^3.0.5",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "concurrently": "^9.2.1",
    "gsap": "^3.13.0",
    "hamo": "1.0.0-dev.6",
    "just-debounce-it": "^3.2.0",
    "lenis": "^1.3.11",
    "next": "^15.6.0-canary.38",
    "next-sanity": "^11.4.2",
    "postprocessing": "^6.37.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sanity-plugin-link-field": "^1.4.2",
    "server-only": "^0.0.1",
    "tempus": "1.0.0-dev.12",
    "three": "^0.180.0",
    "tunnel-rat": "^0.1.2",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@csstools/postcss-global-data": "^3.1.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@sanity/vision": "^4.10.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@theatre/core": "^0.7.2",
    "@theatre/studio": "^0.7.2",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.2.23",
    "@types/node": "^24.6.1",
    "@types/react": "^19.1.17",
    "@types/three": "^0.180.0",
    "cross-env": "^10.1.0",
    "cssnano": "^7.1.1",
    "lefthook": "^1.13.6",
    "postcss-functions": "^4.0.2",
    "postcss-preset-env": "^10.4.0",
    "sanity": "^4.10.2",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@tailwindcss/oxide",
    "esbuild",
    "lefthook",
    "sharp"
  ]
}
